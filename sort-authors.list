#!/bin/bash -e

(
   grep "^Name " authors.list
   grep -v "^Name " authors.list | sed \
      -e "s/^\([^#].*https:\/\/simula.no\/people\/dreibh.*\)$/#\1/g" \
      -e "s/^#\(.*https:\/\/www.uni-due.de\/~be0001\/.*\)$/\1/g" | env LANG=C sort -u
